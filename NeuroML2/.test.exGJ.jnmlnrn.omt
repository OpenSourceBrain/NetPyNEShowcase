# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation
 
target: LEMS_GJ.xml 
engine: jNeuroML_NEURON
mep: .test.exGJ.mep
experiments:
  cell0:
    observables:
      spike times:
        file: 
          path: ex19_v.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
          threshold: -69.5
        tolerance: 0.0
  cell1:
    observables:
      spike times:
        file: 
          path: ex19_v.dat
          columns: [0,2]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
          threshold: -69.5
        tolerance: 0.0
  
