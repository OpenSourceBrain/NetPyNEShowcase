# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation
 
target: LEMS_HybridSmall.xml 
engine: jNeuroML_NEURON
mep: .test.hyizh.mep
experiments:
  v:
    observables:
      spike times:
        file: 
          path: Sim_HybridSmall.PYR_Izhi.v.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
        tolerance: 0.00
