# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation
 
target: LEMS_HHSmall.xml 
engine: jNeuroML
mep: .test.hh.mep
experiments:
  v0:
    observables:
      spike times:
        file: 
          path: Sim_HHSmall.PYR.v.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
        tolerance: 2.2683452421308973e-05
  v1:
    observables:
      spike times:
        file: 
          path: Sim_HHSmall.PYR.v.dat
          columns: [0,2]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
        tolerance: 4.391550656532829e-05
  v2:
    observables:
      spike times:
        file: 
          path: Sim_HHSmall.PYR.v.dat
          columns: [0,3]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
        tolerance: 2.247090018411321e-05
