# Script for running automated tests on OSB, see https://github.com/OpenSourceBrain/osb-model-validation

target: LEMS_SimExampleGJ.xml
engine: jNeuroML
mep: .test.exGJ.mep
experiments:
  pre0:
    observables:
      spike times:
        file:
          path: SimExampleGJ.PopPre.v.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection:
          method: threshold
          threshold: -52
        tolerance: 0.00024931438544009655
  post1:
    observables:
      spike times:
        file:
          path: SimExampleGJ.PopPost.v.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection:
          method: threshold
          threshold: -52
        tolerance: 0.0004739336492890726
  post2:
    observables:
      spike times:
        file:
          path: SimExampleGJ.PopPost.v.dat
          columns: [0,2]
          scaling: [1000, 1000]
        spike detection:
          method: threshold
          threshold: -52
        tolerance: 0.0007165034631000988
