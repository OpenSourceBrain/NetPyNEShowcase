target: HH_run.py 
engine: NetPyNE
mep: ../../NeuroML2/.test.hh.mep
experiments:
  v0:
    observables:
      spike times:
        file: 
          path: Vsoma_cell_0.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
        tolerance: 0.001

  v1:
    observables:
      spike times:
        file: 
          path: Vsoma_cell_1.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
        tolerance: 0.001

  v2:
    observables:
      spike times:
        file: 
          path: Vsoma_cell_2.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
        tolerance: 0.001
